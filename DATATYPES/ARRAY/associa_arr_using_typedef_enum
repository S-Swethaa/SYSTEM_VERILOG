module associa_arr_using_typedef_enum;
typedef enum{trans,receive,repeater}tx;
bit [7:0]arr_enum[tx];
bit [7:0]arr[int];

initial begin
arr_enum[trans]=50;
arr_enum[receive]=20;
arr_enum[repeater]=100;

foreach(arr_enum[i])begin
$display("array_enum values[%s]=%0d",i.name,arr_enum[i]);end
arr[15]=25;
arr[3]=70;
arr[49]=49;
arr[33]=33;
foreach(arr[i])begin
$display("arr[%0d]=%0d",i,arr[i]);
end
end
endmodule


