module packed_structure;
typedef struct {
string name;
bit[6:0] marks;
integer id;
}stu_detail;

initial begin
stu_detail st1,st2;
st1.name="swetha";
st1.marks='d50;
st1.id='d1234;
$display("st1:%p",st1);


st2.name="varsha";
st2.marks='d55;
st2.id='d4321;
$display("st2:%p",st2);
end
endmodule

output
# st1:'{name:"swetha", marks:50, id:1234}
# st2:'{name:"varsha", marks:55, id:4321}
